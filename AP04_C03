#include <iostream>
using namespace std;

int main ()
{
	double nilai;
	char indeks;
	cout<<"masukkan nilai yang diperoleh : ";
	cin>>nilai;

	//melakukan konversi nilai menjadi nilai indeks
	if (nilai>= 85) 
	{
		indeks = 'A';
	}
	else if (nilai >=70)
	{
		indeks = 'B';
	}
	else if (nilai >=55)
	{
		indeks = 'C';
	}
	else if (nilai >=40)
	{
		indeks = 'D';
	}
	else 
	{
		indeks = 'E';
	}

	//menampilkan nilai indeks yang didapatkan
	cout<<"nilai indeks dari nilai"<< " " <<nilai<< " " <<"adalah" << " " << indeks<<endl;

	system ("pause");
	return 0;


}
